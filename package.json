{
  "name": "xmind-sdk-js",
  "version": "1.0.1",
  "description": "The SDK of XMind Ltd.",
  "main": "dist/index.js",
  "browser": "",
  "scripts": {
    "test": "nyc mocha --opts test/mocha.opts",
    "build": "rm -rf ./dist && node_modules/.bin/tsc && npm run browserify",
    "lint": "node_modules/.bin/tslint --config tslint.json --project .",
    "fix": "node_modules/.bin/tslint --config tslint.json --project . --fix",
    "build-doc": "BUILD_DOCS=true node scripts/icons.js",
    "browserify": "node_modules/.bin/browserify ./dist/index.js -o ./dist/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xmindltd/xmind-sdk-js.git"
  },
  "keywords": [
    "xmind-sdk",
    "xmindsdk",
    "xmind"
  ],
  "dependencies": {
    "backbone": "^1.4.0",
    "debug": "^4.1.1",
    "jszip": "^3.2.1",
    "lodash": "^4.17.11",
    "xmind-model": "git+https://gitlab.xmind.cn/daniels/xmind-model.git"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/backbone": "^1.4.0",
    "@types/chai": "^4.1.7",
    "@types/debug": "^4.1.4",
    "@types/jszip": "^3.1.5",
    "@types/lodash": "^4.14.124",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/eslint-plugin-tslint": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "browserify": "^16.2.3",
    "chai": "^4.2.0",
    "jquery": "^3.4.1",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "source-map-support": "^0.5.12",
    "tsify": "^4.0.1",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "uuid": "^3.3.2"
  },
  "author": "daniels",
  "license": "MIT"
}
